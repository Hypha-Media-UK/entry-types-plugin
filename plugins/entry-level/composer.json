  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "entry-types",
    "structure",
    "parent",
    "hierarchy"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hypha Media",
      "homepage": "https://hyphamedia.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "craftcms/cms": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "hyphamedia\\entrylevel\\": "src/"
    }
  },
  "extra": {
    "name": "Entry Level",
    "handle": "entry-level",
    "hasCpSettings": true,
    "class": "hyphamedia\\entrylevel\\Plugin"
