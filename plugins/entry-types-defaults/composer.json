{
  "name": "hyphamedia/entry-types-defaults",
  "description": "Automatically set default parent entries based on Entry Type hierarchy configuration",
  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "entry-types",
    "structure",
    "parent",
    "hierarchy"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hypha Media",
      "homepage": "https://hyphamedia.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "craftcms/cms": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "hyphamedia\\entrytypesdefaults\\": "src/"
    }
  },
  "extra": {
    "name": "Entry Types Defaults",
    "handle": "entry-types-defaults",
    "hasCpSettings": true,
    "class": "hyphamedia\\entrytypesdefaults\\Plugin"
  }
}

